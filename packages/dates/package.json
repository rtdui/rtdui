{
  "name": "@rtdui/dates",
  "version": "5.0.0",
  "description": "Calendars, date and time pickers based on Rtdui components",
  "types": "./lib/index.d.ts",
  "main": "./cjs/index.cjs",
  "module": "./esm/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./esm/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./cjs/index.cjs"
      }
    }
  },
  "keywords": [
    "react",
    "components",
    "library",
    "ui",
    "ui-kit",
    "calendar",
    "date",
    "date-picker",
    "time",
    "time-picker"
  ],
  "author": "fu zhong wei <jackyleefzw@163.com>",
  "license": "MIT",
  "homepage": "https://rtdui.com/",
  "sideEffects": [
    "*.css"
  ],
  "repository": {
    "url": "https://github.com/rtdui/rtdui.git",
    "type": "git",
    "directory": "packages/dates"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "peerDependencies": {
    "@rtdui/core": "5.0.0",
    "@rtdui/hooks": "5.0.0",
    "react": "^18.x || ^19.x",
    "react-dom": "^18.x || ^19.x"
  },
  "dependencies": {
    "@tabler/icons-react": "^3.5.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.3.0",
    "date-fns-tz": "^3.1.3",
    "keen-slider": "^6.8.6"
  }
}

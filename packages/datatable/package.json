{
  "name": "@rtdui/datatable",
  "version": "4.5.0",
  "description": "React DataTable component based on Rtdui components",
  "types": "./lib/index.d.ts",
  "main": "./cjs/index.cjs",
  "module": "./esm/index.mjs",

{
  "name": "@rtdui/qr-code",
  "version": "1.0.5",
  "description": "React QR Code component",
  "types": "./lib/index.d.ts",
  "main": "./cjs/index.cjs",
  "module": "./esm/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./esm/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./cjs/index.cjs"
      }
    },
    "./styles.css": "./styles.css",
    "./styles.layer.css": "./styles.layer.css",
    "./styles/*": "./styles/*"
  },
  "keywords": [
    "hooks",
    "react",
    "remix",
    "remixjs",
    "next",
    "nextjs",
    "ui",
    "ui-kit",
    "qr",
    "qrcode",
    "frontend",
    "design"
  ],
  "author": "fu zhong wei <jackyleefzw@163.com>",
  "license": "MIT",
  "homepage": "https://rtdui.com/",
  "repository": {
    "url": "https://github.com/rtdui/rtdui.git",
    "type": "git",
    "directory": "packages/qr-code"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {
    "qrcode.react": "^3.1.0"
  },
  "peerDependencies": {
    "@rtdui/core": "1.0.5",
    "@rtdui/hooks": "1.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}

  ],
  "repository": {
    "url": "https://github.com/rtdui/rtdui.git",
    "type": "git",
    "directory": "packages/shiki-highlight"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "peerDependencies": {
    "@rtdui/core": "5.1.0",
    "@rtdui/hooks": "5.1.0",
    "react": "^18.x || ^19.x",
    "react-dom": "^18.x || ^19.x"
  },
  "dependencies": {
    "@shikijs/colorized-brackets": "^3.2.1",

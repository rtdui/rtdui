      "require": {
        "types": "./lib/index.d.ts",
        "default": "./cjs/index.cjs"
      }
    }
  },
  "keywords": [
    "react",
    "components",
    "library",
    "ui",
    "ui-kit",
    "highlight",
    "shiki"
  ],
  "author": "fu zhong wei <jackyleefzw@163.com>",
  "license": "MIT",
  "homepage": "https://rtdui.com/",
